// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Service {
  id          String   @id @default(cuid()) @map("_id")
  title       String
  slug        String?  @unique
  description String
  image       String?
}